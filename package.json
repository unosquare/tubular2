{
    "version": "1.0.0",
    "name": "tubular2",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/unosquare/tubular2.git"
    },
    "scripts": {
        "test": "karma start karma.conf.js",
        "test-once": "tsc && karma start karma.conf.js --single-run",
        "preweb-server": "gulp --gulpfile ./src/Unosquare.Tubular2.Web/gulpfile.js build",
        "web-server": "http-server ./src/Unosquare.Tubular2.Web/wwwroot",
        "pree2e": "npm run webdriver:update",
        "e2e": "gulp e2e && concurrently \"http-server ./src/Unosquare.Tubular2.Web/wwwroot\" \"protractor protractor.config.js\" --kill-others --success first",
        "webdriver:update": "webdriver-manager update"
    },
    "dependencies": {
        "@angular/common": "2.0.0",
        "@angular/compiler": "2.0.0",
        "@angular/core": "2.0.0",
        "@angular/forms": "2.0.0",
        "@angular/http": "2.0.0",
        "@angular/platform-browser": "2.0.0",
        "@angular/platform-browser-dynamic": "2.0.0",
        "@angular/router": "3.0.0",
        "@angular/upgrade": "2.0.0",
        "core-js": "^2.4.1",
        "reflect-metadata": "^0.1.3",
        "rxjs": "5.0.0-beta.12",
        "systemjs": "0.19.27",
        "zone.js": "^0.6.23",
        "moment": "2.15.2",
        "@ng-bootstrap/ng-bootstrap": "1.0.0-alpha.4",
        "filesaver.js": "0.2.0"
    },
    "devDependencies": {
        "@types/core-js": "^0.9.34",
        "@types/jasmine": "^2.5.35",
        "@types/node": "^6.0.45",
        "@types/selenium-webdriver": "^2.53.32",
        "@types/protractor": "1.5.20",
        "concurrently": "3.1.0",
        "del": "2.2.0",
        "gulp": "3.9.0",
        "gulp-typescript": "^3.0.2",
        "http-server": "0.9.0",
        "jasmine-core": "2.5.0",
        "karma": "^1.3.0",
        "karma-chrome-launcher": "^2.0.0",
        "karma-cli": "^1.0.1",
        "karma-jasmine": "^1.0.2",
        "protractor": "^4.0.9",
        "ts-loader": "^0.8.2",
        "typescript": "^2.0.3"
    }
}